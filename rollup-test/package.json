{
  "name": "rollup-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^17.0.0",
    "esbuild": "^0.12.28",
    "rollup": "^2.56.3",
    "snowpack": "^3.8.8"
  },
  "scripts": {
    "build:rollup": "rollup src/index.js --file dist/rollup.js --format iife",
    "build:parcel": "parcel build src/index.js --out-dir dist --out-file parcel.js --no-minify",
    "build:esbuild": "esbuild src/index.js --bundle --outfile=dist/esbuild.js --format=iife",
    "build:browserify": "browserify src/index.js -o dist/browserify.js -t [ babelify --presets [ \"es2015\" ]"
  }
}
